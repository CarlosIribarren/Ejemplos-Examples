<web-app 
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
    version="3.0">

  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
  	<servlet-name>EmpleadoServlet</servlet-name>
  	
  	
  	<servlet-class>my.project.controller.EmpleadoServlet</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>CallEJBServlet</servlet-name>
  	
  	
  	<servlet-class>my.project.controller.CallEJBServlet</servlet-class>
  </servlet>


	<servlet-mapping>
		<servlet-name>CallEJBServlet</servlet-name>
		<url-pattern>/CallEJBServlet</url-pattern>
	</servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>EmpleadoServlet</servlet-name>
  	<url-pattern>/EmpleadoServlet</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
</web-app>
