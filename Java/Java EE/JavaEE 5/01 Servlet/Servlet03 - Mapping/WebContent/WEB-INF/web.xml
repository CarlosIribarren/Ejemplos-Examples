<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>Servlet03 - Mapping</display-name>
  <welcome-file-list>
    <welcome-file>inicio</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>P1</servlet-name>
    <servlet-class>PantallaInicio</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>P1</servlet-name>
    <url-pattern>/inicio</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>P2</servlet-name>
    <servlet-class>PantallaSegunda</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>P2</servlet-name>
    <url-pattern>/segundaPantalla</url-pattern>
  </servlet-mapping>
</web-app>